.average
  add RA R0
  bsr RA
  ret

.cmp_test
  ser RA #111
  ret

.loop_test         ;BEGIN LoopTest
  ser RA #007      ;  let num1 = 1
  ser R0 #002      ;  let num2 = 10
  .while           ;  WHILE num1 < num2
    cmp RA R0
    jle .endwhile
    dec RA         ;    decrement num1
    jmp .while
  .endwhile        ;  ENDWHILE
  ret              ;END LoopTest

.start
  ser RA $111
  ser R0 $101
  ; fnc .loop_test
  orr RA R0
  otr RA
  ret